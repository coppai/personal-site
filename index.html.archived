    <script>
    $(document).ready(function() {

        var quoteGenerator = {
            quotes: [{
                quote: 'One machine can do the work of fifty ordinary men. No machine can do the work of one extraordinary man.',
                name: 'Elbert Hubbard'
            }, {
                quote: "If I had an hour to solve a problem I'd spend 55 minutes thinking about the problem and 5 minutes thinking about solutions.",
                name: 'Albert Einstein'
            }, {
                quote: 'First, solve the problem. Then, write the code.',
                name: 'John Johnson'
            }],
            // returns a random quote from the array of quote objects
            getRandomQuote: function(index) {
                return this.quotes[Math.floor(Math.random() *
                    (this.quotes.length - 0))];
            }
        };

        var quote = quoteGenerator.getRandomQuote();

        $(".quote").text(quote['quote']);
        $(".name").text(quote['name']);

        $(".jumbo-quote").show();

    });
    </script>
